{
  "name": "express-react-template",
  "version": "1.0.0",
  "description": "",
  "main": "api/dist/index.js",
  "engines": {
    "node": "12.22.5"
  },
  "scripts": {
    "install-all": "npm install && npm install --prefix client",
    "start": "node ./api/dist/index.js",
    "server": "nodemon ./api/dist/index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"tsc -w\" \"npm run server\" \"npm run client\"",
    "build": "tsc",
    "heroku-postbuild": "tsc && NPM_CONFIG_PRODUCTION=false npm install --prefix client --force && npm run build --prefix client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.14",
    "typescript": "^4.4.4"
  }
}
